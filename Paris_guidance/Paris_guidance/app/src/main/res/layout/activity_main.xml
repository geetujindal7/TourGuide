<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/paaris"
    tools:context="com.example.hi.paris_guidance.MainActivity">
    <TextView
        style="@style/header"
        android:text="PARIS"
        android:layout_marginLeft="50dp"/>
    <TextView
        style="@style/header"
        android:text="CITY OF LOVE"
        android:layout_marginLeft="230dp"/>
    <TextView
        android:text="CLICK ON ANY:"
        style="@style/header"
        android:layout_marginTop="160dp"
        android:id="@+id/select" />
    <TextView
        style="@style/header"
        android:text="FAMOUS PLACES"
        android:id="@+id/places"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/select"/>
    <TextView
        android:text="CHURCH"
        android:layout_marginTop="25dp"
        android:id="@+id/church"
        android:layout_below="@+id/places"
        style="@style/header"/>
    <TextView
        android:text="EATERIES"
        android:layout_below="@+id/church"
        android:layout_marginTop="25dp"
        android:id="@+id/bars"
        style="@style/header"/>
    <TextView
        android:text="MALLS"
        android:layout_below="@+id/bars"
        android:layout_marginTop="30dp"
        android:id="@+id/hh"
        style="@style/header"/>
</RelativeLayout>